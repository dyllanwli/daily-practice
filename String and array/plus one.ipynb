   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def plusOne(self, digits):\n",
    "        result = 0\n",
    "        for index, i in enumerate(reversed(digits)):\n",
    "            print(index,i)\n",
    "            result += i*10**index\n",
    "        return result"
   ]
  },
