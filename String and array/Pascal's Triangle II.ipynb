   "source": [
    "class Solution:\n",
    "    def getRow(self, rowIndex: int):\n",
    "        row = [1]\n",
    "        for _ in range(rowIndex):\n",
    "            row = [x + y for x, y in zip([0]+row, row+[0])]\n",
    "        return row"
   ]
  },
